        <DatePicker Grid.Row="1" Grid.Column="0" Margin="20"/>
        <Button Grid.Column="1" Grid.Row="1" Margin="30,30.333,299,30.333" Content="Hämta och exportera till excel"/>
        <Button Grid.Column="0" Grid.Row="2" Margin="10,60.667,110,10" Content="Tillbaka"/>
        
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
   </Grid.RowDefinitions>

        
    </Grid>
   
    
